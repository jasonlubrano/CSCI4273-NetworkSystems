all: ./proxyserver.c
	gcc proxyserver.c -pthread -g -o ./webproxy

clean:
	$(RM) *.o ./webproxy


all: ./proxyserver.c
	gcc -Wall proxyserver.c -o ./webproxy -lcrypto -lssl

clean:
	$(RM) *.o ./webproxy


